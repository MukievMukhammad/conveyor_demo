<?xml version="1.0" ?>
<robot name="blue_cube">
  <link name="base_link">
    <inertial>
      <!-- <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/> -->
      <mass value="0.0025"/>
      <inertia ixx="0.000001" ixy="0.0" ixz="0.0" iyy="0.000001" iyz="0.0" izz="0.000001"/>
    </inertial>
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.01"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.01"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="base_link">
    <material>Gazebo/Blue</material>
    <collision name="base_link_collision">
      <surface>
        <friction>
          <ode>
            <mu>30</mu>
            <!-- <mu2>30</mu2> -->
          </ode>
        </friction>
      </surface>
    </collision>
  </gazebo>
</robot>

